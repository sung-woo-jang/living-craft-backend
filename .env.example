# 데이터베이스 설정
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=reservation_user
DB_PASSWORD=reservation_password
DB_DATABASE=reservation_db

# JWT 설정
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h

# 네이버 OAuth 설정
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret
NAVER_CALLBACK_URL=http://localhost:3000/api/auth/callback/naver

# 네이버 클라우드 플랫폼 SMS
NCP_ACCESS_KEY=your-ncp-access-key
NCP_SECRET_KEY=your-ncp-secret-key
NCP_SERVICE_ID=your-ncp-service-id
NCP_CALLING_NUMBER=01012345678

# 네이버 클라우드 플랫폼 이메일
NCP_EMAIL_ACCESS_KEY=your-ncp-email-access-key
NCP_EMAIL_SECRET_KEY=your-ncp-email-secret-key

# 관리자 계정
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123!

# Redis (캐시 및 세션)
REDIS_HOST=localhost
REDIS_PORT=6379

# 파일 업로드 설정
UPLOAD_PATH=./uploads
MAX_FILE_SIZE=10485760

# 애플리케이션 설정
PORT=3000
NODE_ENV=development
