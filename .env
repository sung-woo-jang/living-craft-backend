# Database
DATABASE_TYPE=postgres
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=password
DATABASE_NAME=reservation_db

# JWT
JWT_SECRET=your-super-secret-key-change-this-in-production
JWT_EXPIRES_IN=7d

# Redis (for Bull queue)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# File Upload
MAX_FILE_SIZE=10485760  # 10MB
UPLOAD_DIR=./uploads

# Swagger
SWAGGER_TITLE="예약 서비스 API"
SWAGGER_DESCRIPTION="개인 사업자용 예약 서비스 플랫폼 API"
SWAGGER_VERSION="1.0"

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# Admin credentials
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin123!@#

# Naver OAuth (when implementing later)
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=
NAVER_CALLBACK_URL=

# SMS/Email service config (when implementing later)
SMS_API_KEY=
EMAIL_API_KEY=
